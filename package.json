  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@yamadayuki/ogh-sample": "^0.4.0",
    "codecov": "^3.2.0",
    "lerna": "^3.8.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "@yamadayuki/ogh-sample": {
    "prettier": {
      "pattern": "**/packages/**/*.{js,jsx,ts,tsx}"
    }
  }
